    <IncludePaths Include="DeviceCode\pal\base_tcl\services_common\inc;" />
    <IncludePaths Include="DeviceCode\pal\base_tcl\sample_apps\AppsCommon\inc" />
    <IncludePaths Include="DeviceCode\pal\base_tcl\notification\inc" />

    <HFile Include="spot_alljoyn_native_services.h" />
    <Compile Include="spot_alljoyn_native_services.cpp" />
    <Compile Include="spot_alljoyn_native_services_Microsoft_SPOT_AllJoyn_Services_AJ_Services_mshl.cpp" />
    <HFile Include="spot_alljoyn_native_services_Microsoft_SPOT_AllJoyn_Services_AJ_Services.h" />
    
</ItemGroup>

<Import Project="$(SPOCLIENT)\tools\targets\Microsoft.SPOT.System.Targets" />
</Project>
