<Project DefaultTargets="Build" ToolsVersion="3.5" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
  
    <Description>Alljoyn Services interop library</Description>
    <Level>CLR</Level>
    <Groups>Alljoyn Services</Groups>
  
    <ProjectPath>$(SPOCLIENT)\CLR\Libraries\SPOT_Alljoyn_Services\dotnetmf.proj</ProjectPath>
    <AssemblyName>SPOT_AllJoyn_Services</AssemblyName>
    <LibraryFile>SPOT_Alljoyn_Services.$(LIB_EXT)</LibraryFile>
    <Directory>CLR\Libraries\SPOT_Alljoyn_Services</Directory>
    <ManifestFile>SPOT_Alljoyn_Services.$(LIB_EXT).manifest</ManifestFile>    
    <PlatformIndependent>True</PlatformIndependent>
    <Required>False</Required>
    <IgnoreDefaultLibPath>False</IgnoreDefaultLibPath>
    <IsStub>False</IsStub>
